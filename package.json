{
  "name": "v-oss",
  "version": "0.1.0",
  "private": true,
  "license": "UNLICENSED",
  "main": "electron-dist/main.js",
  "author": {
    "name": "v-oss",
    "email": "support@v-oss.example.com"
  },
  "description": "接骨院向け電子カルテシステム - デスクトップアプリケーション",
  "scripts": {
    "dev": "next dev",
    "dev:free": "DEV_BYPASS_AUTH=true next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "tsx --test test/*.test.ts",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "type-check": "tsc --noEmit",
    "quality": "npm run type-check && npm run lint && npm run format:check",
    "verify": "npm run type-check && npm run lint && npm run build",
    "test:e2e:electron": "npm run electron:build:ts && playwright test -c playwright.config.ts",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev",
    "db:studio": "prisma studio",
    "db:seed": "tsx prisma/seed.ts",
    "electron:dev": "concurrently -k \"HOSTNAME=127.0.0.1 npm run dev\" \"wait-on http://127.0.0.1:3000 && npm run electron:start\"",
    "electron:start": "NODE_ENV=development electron electron-dist/main.js",
    "electron:build:ts": "tsc -p electron/tsconfig.json",
    "electron:build:next": "node scripts/build-next-electron.js",
    "electron:prepare-template-db": "node scripts/prepare-template-db.js",
    "electron:build": "npm run electron:build:ts && npm run electron:build:next && npm run electron:prepare-template-db && node scripts/prepare-appdir.js",
    "electron:pack": "npm run electron:build && electron-builder --dir",
    "electron:dist": "npm run electron:build && electron-builder",
    "electron:dist:publish": "npm run electron:build && electron-builder --publish always",
    "electron:dist:mac": "npm run electron:build && electron-builder --mac",
    "electron:dist:mac:publish": "npm run electron:build && electron-builder --mac --publish always",
    "electron:dist:win": "npm run electron:build && electron-builder --win",
    "electron:dist:win:publish": "npm run electron:build && electron-builder --win nsis --x64 --publish always",
    "electron:preflight:win-store": "node scripts/check-win-store-build-env.js",
    "electron:dist:win:store": "npm run electron:preflight:win-store && npm run electron:build && electron-builder --win appx --x64 --publish never",
    "electron:preflight:win-signed": "node scripts/check-win-signing-env.js",
    "electron:dist:win:signed": "npm run electron:preflight:win-signed && npm run electron:dist:win",
    "electron:dist:linux": "npm run electron:build && electron-builder --linux",
    "postinstall": "electron-builder install-app-deps"
  },
  "dependencies": {
    "@prisma/client": "^6.11.0",
    "@react-three/drei": "^9.111.3",
    "@react-three/fiber": "^8.16.7",
    "@supabase/ssr": "^0.8.0",
    "@supabase/supabase-js": "^2.86.2",
    "@types/archiver": "^7.0.0",
    "archiver": "^7.0.1",
    "bcryptjs": "^3.0.2",
    "better-sqlite3": "^11.10.0",
    "date-fns": "^3.6.0",
    "diff": "^8.0.3",
    "electron-serve": "^2.1.1",
    "electron-updater": "^6.6.2",
    "framer-motion": "^12.23.12",
    "lucide-react": "0.537.0",
    "next": "14.2.35",
    "next-auth": "^4.24.11",
    "pdfkit": "^0.17.2",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "three": "^0.169.0",
    "yauzl": "^3.2.0",
    "zod": "^3.25.74"
  },
  "devDependencies": {
    "@playwright/test": "^1.52.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/diff": "^7.0.2",
    "@types/node": "22.7.5",
    "@types/pdfkit": "^0.17.3",
    "@types/react": "18.3.11",
    "@types/react-dom": "18.3.0",
    "@types/yauzl": "^2.10.3",
    "autoprefixer": "10.4.21",
    "concurrently": "^9.1.2",
    "electron": "^35.2.1",
    "electron-builder": "^26.7.0",
    "eslint": "8.57.1",
    "eslint-config-next": "14.2.35",
    "playwright": "^1.52.0",
    "postcss": "8.5.6",
    "prettier": "3.3.3",
    "prisma": "^6.11.0",
    "tailwindcss": "3.4.17",
    "tsx": "^4.7.0",
    "typescript": "5.6.3",
    "wait-on": "^8.0.3"
  },
  "overrides": {
    "glob": "^11.0.0"
  },
  "build": {
    "extends": "electron-builder.yml"
  }
}
